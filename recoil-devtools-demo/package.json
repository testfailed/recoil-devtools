{
  "name": "recoil-devtools-demo",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:dev": "yarn build --env.env=development",
    "build:dev:watch": "yarn build:dev --watch --hot",
    "build:dev:analyze": "yarn build:dev --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "build:prod": "yarn build -p --env.env=production",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:analyze": "yarn build:prod --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "lint": "eslint ./src --ext .jsx --ext .js && prettier --check \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint ./src --fix --ext .jsx --ext .js --fix && prettier --write \"src/**/*.{ts,tsx}\"",
    "serve:dev": "webpack-dev-server --mode development --open --hot --env.env=development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --env.addon=dashboard",
    "start": "yarn serve:dev",
    "test": "jest --runInBand --detectOpenHandles --config .jest.config.js",
    "test:watch": "jest -u --runInBand --verbose --watch --detectOpenHandles --config .jest.config.js",
    "test:coverage": "jest -u --coverage --verbose --runInBand --detectOpenHandles --config .jest.config.js"
  },
  "dependencies": {
    "i18next": "^19.6.2",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-http-backend": "^1.0.17",
    "moment-timezone": "^0.5.31",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "inject-html-webpack-plugin": "^0.0.39",
    "jest": "^26.1.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "prettier-tslint": "^0.4.2",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^9.0.2",
    "sinon": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "url-loader": "1.1.2",
    "webpack": "^4.43.0",
    "webpack-async-chunk-names-plugin": "^0.1.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.0.9",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
