{
  "name": "recoil-devtools-logger",
  "version": "0.0.7",
  "description": "Logger for Recoil",
  "main": "dist/commonjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "repository": "git@github.com:ulises-jeremias/recoil-devtool.git",
  "author": "ulises-jeremias <ulisescf.24@gmail.com>",
  "license": "MIT",
  "keywords": [
    "recoil",
    "logger",
    "devtools",
    "recoil-devtools",
    "recoil-devtools-logger"
  ],
  "scripts": {
    "format": "prettier --check \"src/**/*.{ts,tsx,js}\"",
    "dev": "webpack-dev-server --inline --hot",
    "build": "yarn build:commonjs && yarn build:es6 && yarn build:amd && yarn build:umd && yarn build:declarations",
    "build:commonjs": "yarn tsc --module commonjs --outDir ./dist/commonjs",
    "build:es6": "yarn tsc --module es6 --outDir ./dist/es6",
    "build:amd": "yarn tsc --module amd --outDir ./dist/amd",
    "build:umd": "webpack -p webpack.config.umd.js",
    "build:declarations": "yarn tsc --declaration --emitDeclarationOnly --outDir ./dist/types"
  },
  "dependencies": {
    "deep-diff": "^1.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "^0.0.10"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "recoil": ">=0.0.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/deep-diff": "^1.0.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "awesome-typescript-loader": "^5.2.1",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "prettier-tslint": "^0.4.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
